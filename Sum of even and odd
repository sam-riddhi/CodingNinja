import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc = new Scanner (System.in);
		int n = sc.nextInt();
		int s1 = 0, s2 = 0;
		while (n > 0)
		{
			int d = n % 10;
			if (d % 2 == 0)
			{
				s1 = s1 + d;
			}
			else
			{
				s2 = s2 + d;
			}
			n = n / 10;
		}
		System.out.print(s1 + " " + s2);
	}
}
